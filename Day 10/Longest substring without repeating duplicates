# Longest substring without repeating duplicates
def lengthOfLongestSubstring(s):
    container = set()
    i = 0
    res = 0

    for j in range(len(s)):
        while s[j] in container:
            container.remove(s[i])
            i += 1
        res = max(res, (j - i) + 1)
        container.add(s[j])
    return res
s = "abcabcbb"
print(lengthOfLongestSubstring(s))
