# bucket sort function for values greater than 0.
def bucket(arr):
    n = len(arr)
    bucket_list = [[] for i in range(n)]
    for val in arr:
        index = int(val * n)
        if index >= 6:
            index = n - 1

        bucket_list[index].append(val)
        for  sublist in bucket_list:
            if len(sublist) > 1:
                sublist.sort()
    res = []
    for i in bucket_list:
        res.extend(i)
    print(res)
arr=[0.34,0.45,0.24,0.50,0.87,0.98,1.09]
bucket(arr)
