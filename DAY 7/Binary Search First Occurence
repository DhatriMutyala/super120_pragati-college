#binary search index of first occurence
def binary_search(nums,k):
    l = 0
    r = len(nums) - 1
    while l <= r:
        mid = (l+r) // 2
        if nums[mid] == k:
            res = mid
            r = mid - 1
        elif nums[mid] < k:
             l = mid + 1
        else:
            r = mid - 1
    return res
nums = [1,2,3,3,5]
k = 3
print(binary_search(nums,k))
