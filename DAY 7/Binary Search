#binary search
def binary_search(nums,k):
    l = 0
    r = len(nums) - 1
    while l <= r:
        mid = (l+r) // 2
        if nums[mid] == k:
           return mid + 1
        if nums[mid] < k:
           l = mid + 1
        else:
            r = mid - 1
    return -1
nums = [1,2,3,4,5]
k = 5
print(binary_search(nums,k))
