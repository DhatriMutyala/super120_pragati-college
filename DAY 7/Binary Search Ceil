#binary search ceil
def ceil(nums,k):
    l,r = 0,len(nums) - 1
    res = -1
    while l <= r:
        mid = (l + r) // 2

        if nums[mid] > k:
            res = mid
            r = mid - 1
        else:
            l = mid + 1
    return nums[res]

nums = [3,4,5,6,12]
k = 4
print(ceil(nums,k))
