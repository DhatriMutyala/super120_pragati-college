#longest consecutive sequence
def longestConsecutive(nums):
    hashset = set(nums)
    res = 0
    for num in hashset:
        if (num - 1) not in hashset:
            count = 0
            while (num + count) in hashset:
                count += 1
            res = max(count, res)
    return res
nums = [100,4,200,1,3,2]
print(longestConsecutive(nums))

