# Valid Anagram
def isAnagram(s,t):
    if len(s) != len(t):
        return False
    countS, countT = {}, {}
    for i in range(len(s)):
        countT[t[i]] = countT.get(t[i], 0) + 1
        countS[s[i]] = countS.get(s[i], 0) + 1
    return countT == countS
s = "anagram"
t = "nagaram"
print(isAnagram(s,t))
