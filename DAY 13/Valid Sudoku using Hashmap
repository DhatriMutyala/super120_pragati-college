# Valid Sudoku using Hashmap
import collections
board = [["5","3",".",".","7",".",".",".","."]
,["6",".",".","1","9","5",".",".","."]
,[".","9","8",".",".",".",".","6","."]
,["8",".",".",".","6",".",".",".","3"]
,["5",".",".","8",".","3",".",".","1"]
,["7",".",".",".","2",".",".",".","6"]
,[".","6",".",".",".",".","2","8","."]
,[".",".",".","4","1","9",".",".","5"]
,[".",".",".",".","8",".",".","7","9"]]
def sudoku(board):
    col = collections.defaultdict(list)
    row = collections.defaultdict(list)
    cube = collections.defaultdict(list)

    for r in range(9):
        for c in range(9):

            val = board[r][c]
            if val == ".":
                continue
            if (val in row[r]) or (val in col[c]) or (val in cube[(r // 3), (c // 3)]):
                return False
            row[r].append(val)
            col[c].append(val)
            cube[(r // 3), (c // 3)].append(val)
    return True


print(sudoku(board))
