#Reorder list (143 LC)
def reorderList(self, head: Optional[ListNode]) -> None:
        """
        Do not return anything, modify head in-place instead.
        """
        if head.next is None:
            return head
        slow = head
        fast = head.next
        while fast and fast.next:
            slow = slow.next
            fast = fast.next.next
        cur = slow.next
        slow.next = None

        prev = None
        while cur:
            next_node = cur.next
            cur.next = prev
            prev = cur
            cur = next_node
        list2 = prev

        res = temp = ListNode(0)
        while head or list2:
            if head:
               res.next = head
               head = head.next
               res = res.next
            if list2:
               res.next = list2
               list2 = list2.next
               res= res.next
        return temp.next


