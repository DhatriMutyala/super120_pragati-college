# LL  every data we insert  will be at begin and display
class Node:
     def __init__(self,data):
         self.data = data
         self.next = None

class Linkedlist:
     def __init__(self):
         self.head = None
         
     def insert(self,data):
         newnode = Node(data)
         if self.head is None:
             self.head = newnode
             return
         newnode.next = self.head
         self.head = newnode

     def display(self):
         if self.head is None:
             print("no data")
         cur = self.head
         while cur:
             print(cur.data, end=" -> ")
             cur = cur.next

linkedlist_1 = Linkedlist()
linkedlist_1.insert(5)
linkedlist_1.insert(6)
linkedlist_1.insert(8)
linkedlist_1.insert(9)
linkedlist_1.display()
