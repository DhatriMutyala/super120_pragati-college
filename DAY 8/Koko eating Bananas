#Koko eating bananas
import math
piles = [3,6,7,11]
h = 8
def minEatingSpeed(piles,h):
    l, r = 1, max(piles)
    ans = 0
    while l <= r:
        mid = (l + r) // 2
        hour = 0
        for banana in piles:
            hour += math.ceil(banana / mid)
        if hour <= h:
            ans = mid
            r = mid - 1
        else:
            l = mid + 1
    return ans
print(minEatingSpeed(piles,h))
